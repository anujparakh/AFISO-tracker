<%= link_to("<< Back to List", semesters_path, :class => 'back-link') %>

<div class="semesters show">
  <h2>Show Semester</h2>

  <table summary="Semester detail view">
    <tr>
      <th>Semester</th>
      <td><%= @semester.semester_name %></td>
    </tr>
    <tr>
      <th>Start Date</th>
      <td><%= @semester.start_date %></td>
    </tr>
    <tr>
      <th>End Date</th>
      <td><%= @semester.end_date %></td>
    </tr>
	<tr>
      <th>Dues Deadline</th>
      <td><%= @semester.dues_deadline %></td>
    </tr>
  </table>

</div>
