<table class="table" summary="Semester form fields">
    <tr>
        <th>Semester Name</th>
        <td><%= f.text_field(:semester_name, id: 'name') %></td>
    </tr>
    <tr>
        <th>Start Date</th>
        <td><%= f.date_select(:start_date,{order: [:month, :day, :year], start_year: Date.today.year + 1, end_year: Date.today.year})%></td>
    </tr>
    <tr>
        <th>End Date</th>
        <td><%= f.date_select(:end_date,{order: [:month, :day, :year], start_year: Date.today.year + 1, end_year: Date.today.year})%></td>
    </tr>
    <tr>
        <th>Membership Dues Deadline</th>
        <td><%= f.date_select(:dues_deadline,{order: [:month, :day, :year], start_year: Date.today.year + 1, end_year: Date.today.year})%></td>
    </tr>
</table>